<?xml version="1.0" encoding="UTF-8"?>
<excel id="user" code="user" name="用户信息导入">
    <colgroup>
        <col index="A" width='17em'></col>
        <col index="B" width='17em'></col>
        <col index="C" width='17em'></col>
        <col index="D" width='17em'></col>
    </colgroup>
    <title>
        <tr height="16px">
            <td rowspan="1" colspan="6" value="学生信息导入" />
        </tr>
    </title>
    <thead>
        <tr height="16px">
        	<th value="编号" />
            <th value="姓名" />
            <th value="密码" />
            <th value="账号" />
        </tr>
    </thead>
    <tbody>
        <tr height="16px" firstrow="2" firstcol="0" repeat="5">
            <td type="numeric" format="##0" isnullable="false"  /><!--用户编号 -->
            <td type="string" isnullable="false" maxlength="255" /><!--姓名 -->
            <td type="string" isnullable="false" maxlength="255" /><!--密码 -->
            <td type="string" isnullable="false" maxlength="255" /><!--账号-->
        </tr>
    </tbody>
</excel>